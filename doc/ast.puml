@startuml

!theme crt-amber

hide circle

package ast {
    class SC {
        +raw Str
        +fly Fly
    }

    class Fly {
        +raw Str
        +pogs Pog[]
    }

    class Pog {
        +raw Str
        +genome Genome[]
    }

    enum Genome {
        DDL
        DML
    }

    enum DML {
        Assign
        Oop
    }

    class Assign {
        +raw Str
    }

    class Oop {
        +raw Str
    }

    class DDL {
        +raw Str
        +bug Bug
    }

    class Bug {
        +raw Str
        +specie Str
    }
}

package parser {
    class Parser
    class Tree
}

parser.Tree --> ast.SC
parser.Tree --> parser.Parser

ast.SC --> ast.Fly

ast.Fly --> ast.Pog

ast.Pog --> ast.Genome

ast.Genome --> ast.DDL
ast.Genome --> ast.DML

ast.DML --> ast.Assign
ast.DML --> ast.Oop

ast.DDL --> ast.Bug

@enduml
